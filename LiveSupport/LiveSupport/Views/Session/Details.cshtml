@model IEnumerable<LiveSupport.Data.Message>

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<table>
    <tr>
        <th>Start Date:</th> <td>@ViewBag.Session.StartDate</td>
    </tr>
    <tr>
        <th>End Date:</th> <td>@ViewBag.Session.EndDate</td>
    </tr>
    <tr>
        <th>Name:</th> <td>@ViewBag.Session.Client</td>
    </tr>
    <tr>
        <th>E-mail:</th> <td>@ViewBag.Session.Email</td>
    </tr>
    <tr>
        <th>Question:</th> <td>@ViewBag.Session.Question</td>
    </tr>
    <tr>
        <th>Country:</th> <td>@ViewBag.Session.Country</td>
    </tr>
    <tr>
        <th>User Agent:</th> <td>@ViewBag.Session.UserAgent</td>
    </tr>
    <tr>
        <th>Current URL:</th> <td>@ViewBag.Session.CurrentURL</td>
    </tr>
</table>


<table>
    <tr>
        <th>
            Text
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Text
        </td>
    </tr>
}

</table>

